  keywords = {fluorescence,cobalamin,intrinsic factor,transcobalamin},
  pages = {4742--4753},
  file = {/Users/colinrathbun/Zotero/storage/WJSJIAD6/Fedosov Sergey N. et al. - 2006 - Application of a fluorescent cobalamin analogue fo.pdf;/Users/colinrathbun/Zotero/storage/RWP7248R/j.1742-4658.2006.05478.html}
