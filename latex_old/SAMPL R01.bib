  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/TXRGHHZS/Geballe and Guthrie - 2012 - The SAMPL3 blind prediction challenge transfer en.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/NKD4K8AR/s10822-012-9568-8.html:text/html}
