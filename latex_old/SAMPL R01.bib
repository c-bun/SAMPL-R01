  month = may,
  year = {2010},
  keywords = {hydration free energy,overview,SAMPL2,tautomer},
  pages = {259--279},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/JG42W9RE/Geballe et al. - 2010 - The SAMPL2 blind prediction challenge introductio.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/QUD9MR2I/s10822-010-9350-8.html:text/html}
}

@article{ellingson_analysis_2010,
