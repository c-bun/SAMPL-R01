@article{hogues_exhaustive_2014,
  title = {Exhaustive Docking and Solvated Interaction Energy Scoring: Lessons Learned from the {{SAMPL4}} Challenge},
  volume = {28},
  issn = {0920-654X, 1573-4951},
  shorttitle = {Exhaustive Docking and Solvated Interaction Energy Scoring},
  doi = {10.1007/s10822-014-9715-5},
  abstract = {We continued prospective assessments of the Wilma\textendash{}solvated interaction energy (SIE) platform for pose prediction, binding affinity prediction, and virtual screening on the challenging SAMPL4 data sets including the HIV-integrase inhibitor and two host\textendash{}guest systems. New features of the docking algorithm and scoring function are tested here prospectively for the first time. Wilma\textendash{}SIE provides good correlations with actual binding affinities over a wide range of binding affinities that includes strong binders as in the case of SAMPL4 host\textendash{}guest systems. Absolute binding affinities are also reproduced with appropriate training of the scoring function on available data sets or from comparative estimation of the change in target's vibrational entropy. Even when binding modes are known, SIE predictions lack correlation with experimental affinities within dynamic ranges below 2 kcal/mol as in the case of HIV-integrase ligands, but they correctly signaled the narrowness of the dynamic range. Using a common protein structure for all ligands can reduce the noise, while incorporating a more sophisticated solvation treatment improves absolute predictions. The HIV-integrase virtual screening data set consists of promiscuous weak binders with relatively high flexibility and thus it falls outside of the applicability domain of the Wilma\textendash{}SIE docking platform. Despite these difficulties, unbiased docking around three known binding sites of the enzyme resulted in over a third of ligands being docked within 2 \AA from their actual poses and over half of the ligands docked in the correct site, leading to better-than-random virtual screening results.},
  language = {en},
  timestamp = {2016-09-23T21:05:38Z},
  number = {4},
  urldate = {2016-09-16},
  journal = {J Comput Aided Mol Des},
  author = {Hogues, Herv{\'e} and Sulea, Traian and Purisima, Enrico O.},
  month = jan,
  year = {2014},
  keywords = {docking,SAMPL,SAMPL4},
  pages = {417--427},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/D547S7FW/Hogues et al. - 2014 - Exhaustive docking and solvated interaction energy.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/6I7JRX2R/s10822-014-9715-5.html:text/html}
}

@article{guthrie_sampl4_2014,
  title = {{{SAMPL4}}, a Blind Challenge for Computational Solvation Free Energies: The Compounds Considered},
  volume = {28},
  issn = {0920-654X, 1573-4951},
  shorttitle = {{{SAMPL4}}, a Blind Challenge for Computational Solvation Free Energies},
  doi = {10.1007/s10822-014-9738-y},
  abstract = {For the fifth time I have provided a set of solvation energies (1 M gas to 1 M aqueous) for a SAMPL challenge. In this set there are 23 blind compounds and 30 supplementary compounds of related structure to one of the blind sets, but for which the solvation energy is readily available. The best current values of each compound are presented along with complete documentation of the experimental origins of the solvation energies. The calculations needed to go from reported data to solvation energies are presented, with particular attention to aspects which are new to this set. For some compounds the vapor pressures (VP) were reported for the liquid compound, which is solid at room temperature. To correct from VPsubcooled liquid to VPsublimation requires $\Delta$Sfusion, which is only known for mannitol. Estimated values were used for the others, all but one of which were benzene derivatives and expected to have very similar values. The final compound for which $\Delta$Sfusion was estimated was menthol, which melts at 42 $^\circ$C so that modest errors in $\Delta$Sfusion will have little effect. It was also necessary to look into the effects of including estimated values of $\Delta$Cp on this correction. The approximate sizes of the effects of inclusion of $\Delta$Cp in the correction from VPsubcooledliquid to VPsublimation were estimated and it was noted that inclusion of $\Delta$Cp invariably makes $\Delta$GS more positive. To extend the set of compounds for which the solvation energy could be calculated we explored the use of boiling point (b.p.) data from Reaxys/Beilstein as a substitute for studies of the VP as a function of temperature. B.p. data are not always reliable so it was necessary to develop a criterion for rejecting outliers. For two compounds (chlorinated guaiacols) it became clear that inclusion represented overreach; for each there were only two independent pressure, temperature points, which is too little for a trustworthy extrapolation. For a number of compounds the extrapolation from lowest temperature at which the VP was reported to 25 $^\circ$C was long (sometimes over 100$^\circ$) so that it was necessary to consider whether $\Delta$Cp might have significant effects. The problem is that there are no experimental values and possible intramolecular hydrogen bonds make estimation uncertain in some cases. The approximate sizes of the effects of $\Delta$Cp were estimated, and it was noted that inclusion of $\Delta$Cp in the extrapolation of VP down to room temperature invariably makes $\Delta$Gs more negative.},
  language = {en},
  timestamp = {2016-09-23T21:05:38Z},
  number = {3},
  urldate = {2016-09-16},
  journal = {J Comput Aided Mol Des},
  author = {Guthrie, J. Peter},
  month = apr,
  year = {2014},
  keywords = {experiment,hydration free energy,SAMPL4},
  pages = {151--168},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/6GFS29VN/Guthrie - 2014 - SAMPL4, a blind challenge for computational solvat.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/DQR5HFPD/s10822-014-9738-y.html:text/html}
}

@article{genheden_extensive_2014,
  title = {Extensive All-Atom {{Monte Carlo}} Sampling and {{QM}}/{{MM}} Corrections in the {{SAMPL4}} Hydration Free Energy Challenge},
