  journal = {J Comput Aided Mol Des},
  author = {Kumar, Ashutosh and Zhang, Kam Y. J.},
  month = jan,
  year = {2012},
  keywords = {Maybridge,RosettaLigand,SAMPL,SAMPL3,trypsin},
  pages = {603--616},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/6AXKEHVC/Kumar and Zhang - 2012 - Computational fragment-based screening using Roset.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/UN97RXD8/s10822-011-9523-0.html:text/html}
