  author = {Muddana, Hari S. and Sapra, Neil V. and Fenley, Andrew T. and Gilson, Michael K.},
  month = jan,
  year = {2014},
  keywords = {alchemical,hydration free energy,SAMPL,SAMPL4},
  pages = {277--287},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/QQX4UCEQ/Muddana et al. - 2014 - The SAMPL4 hydration challenge evaluation of part.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/VC65C3UT/s10822-014-9714-6.html:text/html}
