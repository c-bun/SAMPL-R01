  year = {2012},
  keywords = {CB7,host–guest complexation,host-guest systems,MMFF,PB,SAMPL,SAMPL3},
  pages = {577--582},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/TQ4ANFJD/Hamaguchi et al. - 2012 - Force-field and quantum-mechanical binding study o.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/QEQRCAM5/s10822-012-9553-2.html:text/html}
