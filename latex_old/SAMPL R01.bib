  keywords = {alchemical,Dielectric constant,explicit solvent,logD,logP,SAMPL,SAMPL5},
  pages = {1--7},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/2Z4T54Q3/Paranahewage et al. - 2016 - Predicting water-to-cyclohexane partitioning of th.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/T5HHN5J3/s10822-016-9950-z.html:text/html}
