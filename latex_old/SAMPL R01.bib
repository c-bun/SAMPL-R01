  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/9Z5RFWZW/Colas and Iorga - 2014 - Virtual screening of the SAMPL4 blinded HIV integr.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/Q7H7GQWN/s10822-014-9707-5.html:text/html}
}

@article{carnegie_anion_2014,
  title = {Anion {{Complexation}} and {{The Hofmeister Effect}}},
