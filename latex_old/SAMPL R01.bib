  year = {2012},
  pages = {325--332},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/AXQH57R8/Ma et al. - 2012 - Acyclic cucurbituril congener binds to local anaes.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/663VH3SZ/10610278.2012.html:text/html}
