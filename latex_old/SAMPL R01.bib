  author = {Beckstein, Oliver and Fourrier, Ana{\"\i}s and Iorga, Bogdan I.},
  month = feb,
  year = {2014},
  keywords = {alchemical,hydration free energy,OPLS-AA,SAMPL,SAMPL4},
  pages = {265--276},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/58W696EQ/Beckstein et al. - 2014 - Prediction of hydration free energies for the SAMP.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/J2RIJ4Q4/s10822-014-9727-1.html:text/html}
