  keywords = {affinity pairs,avidin–biotin,CB7,cucurbit[n]uril,experiment,host–guest complexation,host-guest systems,X-ray diffraction},
  pages = {988--993},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/AII8TWN7/Cao et al. - 2014 - Cucurbit[7]uril⋅Guest Pair with an Attomolar Disso.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/92NG6WNK/abstract.html:text/html}
