  author = {Coleman, Ryan G. and Sterling, Teague and Weiss, Dahlia R.},
  month = feb,
  year = {2014},
  keywords = {HIV integrase,hydration free energy,SAMPL,SAMPL4},
  pages = {201--209},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/4UPP8C2S/Coleman et al. - 2014 - SAMPL4 &amp\; DOCK3.7 lessons for automated dockin.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/ID5QCE8F/s10822-014-9722-6.html:text/html}
