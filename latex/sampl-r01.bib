  keywords = {alchemical,AMOEBA,fixed-charge force field,log P,Molecular dynamics,polarizable force field,solvation free energies},
  pages = {n/a--n/a},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/M974PANV/Mohamed et al. - 2016 - Evaluation of solvation free energies for small mo.pdf:application/pdf}
