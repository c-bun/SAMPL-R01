  month = aug,
  year = {2016},
  keywords = {alchemical,logD,logP,OPLS-AA,SAMPL,SAMPL5},
  pages = {1--14},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/USJ8QBSZ/Kenney et al. - 2016 - Prediction of cyclohexane-water distribution coeff.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/6XBZG2JE/s10822-016-9949-5.html:text/html},
