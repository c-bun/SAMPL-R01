  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/AD6ZW4TV/Carnegie et al. - 2014 - Anion Complexation and The Hofmeister Effect.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/C83PMKB4/abstract.html:text/html},
  groups = {OctaAcid}
}

