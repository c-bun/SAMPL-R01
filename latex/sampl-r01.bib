  journal = {J Comput Aided Mol Des},
  author = {Ribeiro, Raphael F. and Marenich, Aleksandr V. and Cramer, Christopher J. and Truhlar, Donald G.},
  month = apr,
  year = {2010},
  keywords = {hydration free energy,QM,SAMPL,SAMPL2,SMD,tautomer},
