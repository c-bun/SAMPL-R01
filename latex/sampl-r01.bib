  number = {2},
  urldate = {2017-06-22},
  journal = {Nat Biotech},
  author = {Eiben, Christopher B. and Siegel, Justin B. and Bale, Jacob B. and Cooper, Seth and Khatib, Firas and Shen, Betty W. and Players, Foldit and Stoddard, Barry L. and Popovic, Zoran and Baker, David},
  month = feb,
  year = {2012},
  keywords = {Computational biology and bioinformatics,crowdsourcing,Foldit,Molecular engineering,Nanobiotechnology},
  pages = {190--192},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/WQD7QBUS/Eiben et al. - 2012 - Increased Diels-Alderase activity through backbone.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/PRXE5W8D/nbt.2109.html:text/html},
