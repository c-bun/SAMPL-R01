  keywords = {free-energy calculations,GROMOS,hydration free energy,one-step perturbation,relative free energy,replica exchange,residue mutation,solvation free energy,thermodynamic integration},
  pages = {n/a--n/a},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/WXCZKB38/Graf et al. - 2016 - Free-energy calculations of residue mutations in a.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/BQXBPZWH/abstract.html:text/html},
