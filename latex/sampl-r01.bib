  file = {ACS Full Text PDF w/ Links:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/WJTIA88U/Johnson - 2008 - The Trouble with QSAR (or How I Learned To Stop Wo.pdf:application/pdf;ACS Full Text Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/GRCPDXII/ci700332k.html:text/html},
  groups = {Machine learning}
}

@article{Heo:2017:J.Chem.Inf.Model.,
  title = {Protein {{Loop Structure Prediction Using Conformational Space Annealing}}},
  issn = {1549-9596},
  doi = {10.1021/acs.jcim.6b00742},
  abstract = {We have developed a protein loop structure prediction method by combining a new energy function, which we call EPLM (energy for protein loop modeling), with the conformational space annealing (CSA) global optimization algorithm. The energy function includes stereochemistry, dynamic fragment assembly, distance-scaled finite ideal gas reference (DFIRE), and generalized orientation- and distance-dependent terms. For the conformational search of loop structures, we used the CSA algorithm, which has been quite successful in dealing with various hard global optimization problems. We assessed the performance of EPLM with two widely used loop-decoy sets, Jacobson and RAPPER, and compared the results against the DFIRE potential. The accuracy of model selection from a pool of loop decoys as well as de novo loop modeling starting from randomly generated structures was examined separately. For the selection of a nativelike structure from a decoy set, EPLM was more accurate than DFIRE in the case of the Jacobson set and had similar accuracy in the case of the RAPPER set. In terms of sampling more nativelike loop structures, EPLM outperformed EDFIRE for both decoy sets. This new approach equipped with EPLM and CSA can serve as the state-of-the-art de novo loop modeling method.},
  timestamp = {2017-04-19T17:30:32Z},
  urldate = {2017-04-19},
  journal = {J. Chem. Inf. Model.},
  author = {Heo, Seungryong and Lee, Juyong and Joo, Keehyoung and Shin, Hang-Cheol and Lee, Jooyoung},
  month = apr,
  year = {2017},
  keywords = {loop modeling,loop prediction,protein preparation},
  file = {ACS Full Text PDF w/ Links:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/GKFDJDAK/Heo et al. - 2017 - Protein Loop Structure Prediction Using Conformati.pdf:application/pdf;ACS Full Text Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/GARFDCHI/acs.jcim.html:text/html},
  groups = {Loop modeling,Loop modeling}
}

@article{Drwal:2017:J.Chem.Inf.Model.,
  title = {Do {{Fragments}} and {{Crystallization Additives Bind Similarly}} to {{Drug}}-like {{Ligands}}?},
  issn = {1549-9596},
  doi = {10.1021/acs.jcim.6b00769},
  abstract = {The success of fragment-based drug design (FBDD) hinges upon the optimization of low-molecular-weight compounds (MW $<$ 300 Da) with weak binding affinities to lead compounds with high affinity and selectivity. Usually, structural information from fragment\textendash{}protein complexes is used to develop ideas about the binding mode of similar but drug-like molecules. In this regard, crystallization additives such as cryoprotectants or buffer components, which are highly abundant in crystal structures, are frequently ignored. Thus, the aim of this study was to investigate the information present in protein complexes with fragments as well as those with additives and how they relate to the binding modes of their drug-like counterparts. We present a thorough analysis of the binding modes of crystallographic additives, fragments, and drug-like ligands bound to four diverse targets of wide interest in drug discovery and highly represented in the Protein Data Bank: cyclin-dependent kinase 2, $\beta$-secretase 1, carbonic anhydrase 2, and trypsin. We identified a total of 630 unique molecules bound to the catalytic binding sites, among them 31 additives, 222 fragments, and 377 drug-like ligands. In general, we observed that, independent of the target, protein\textendash{}fragment interaction patterns are highly similar to those of drug-like ligands and mostly cover the residues crucial for binding. Crystallographic additives are also able to show conserved binding modes and recover the residues important for binding in some of the cases. Moreover, we show evidence that the information from fragments and drug-like ligands can be applied to rescore docking poses in order to improve the prediction of binding modes.},
  timestamp = {2017-04-26T21:05:15Z},
  urldate = {2017-04-26},
  journal = {J. Chem. Inf. Model.},
  author = {Drwal, Malgorzata N. and Jacquemard, C{\'e}lien and Perez, Carlos and Desaphy, J{\'e}r{\'e}my and Kellenberger, Esther},
  month = apr,
  year = {2017},
  keywords = {additives,binding sites,co-solvents,fragment binding},
  file = {ACS Full Text PDF w/ Links:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/EAIP28S8/Drwal et al. - 2017 - Do Fragments and Crystallization Additives Bind Si.pdf:application/pdf;ACS Full Text Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/9HMNJKBB/acs.jcim.html:text/html},
  groups = {Binding}
}

