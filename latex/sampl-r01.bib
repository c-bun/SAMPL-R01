}

@article{lawrenz_thermodynamic_2012,
  title = {Thermodynamic Integration to Predict Host-Guest Binding Affinities},
  volume = {26},
  issn = {0920-654X, 1573-4951},
  doi = {10.1007/s10822-012-9542-5},
  abstract = {An alchemical free energy method with explicit solvent molecular dynamics simulations was applied as part of the blind prediction contest SAMPL3 to calculate binding free energies for seven guests to an acyclic cucurbit-[n]uril host. The predictions included determination of protonation states for both host and guests, docking pose generation, and binding free energy calculations using thermodynamic integration. We found a root mean square error (RMSE) of 3.6kcalmol-13.6kcalmol-13.6$\backslash$,$\backslash$hbox\{kcal\}$\backslash$,$\backslash$hbox\{mol\}\^\{-1\} from the reference experimental results, with an R2 correlation of 0.51. The agreement with experiment for the largest contributor to this error, guest 6, is improved by 1.7kcalmol-11.7kcalmol-11.7$\backslash$,$\backslash$hbox\{kcal\}$\backslash$,$\backslash$hbox\{mol\}\^\{-1\} when a periodicity-induced free energy correction is applied. The corrections for the other ligands were significantly smaller, and altogether the RMSE was reduced by 0.4kcalmol-10.4kcalmol-10.4 $\backslash$,$\backslash$hbox\{kcal\}$\backslash$,$\backslash$hbox\{mol\}\^\{-1\}. We link properties of the host-guest systems during simulation to errors in the computed free energies. Overall, we show that charged host-guest systems studied here, initialized in unconfirmed docking poses, present a challenge to accurate alchemical simulation methods.},
  language = {en},
  timestamp = {2016-09-16T18:59:32Z},
  number = {5},
  urldate = {2016-09-16},
  journal = {J Comput Aided Mol Des},
  author = {Lawrenz, Morgan and Wereszczynski, Jeff and Ortiz-S{\'a}nchez, Juan Manuel and Nichols, Sara E. and McCammon, J. Andrew},
  month = feb,
  year = {2012},
  keywords = {alchemical,CBClip,SAMPL,SAMPL3},
  pages = {569--576},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/B4P7ZDZN/Lawrenz et al. - 2012 - Thermodynamic integration to predict host-guest bi.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/EU9FKAB3/s10822-012-9542-5.html:text/html},
  groups = {SAMPL R01,SAMPL R01,SAMPL}
}

@article{sulea_exhaustive_2011,
  title = {Exhaustive Search and Solvated Interaction Energy ({{SIE}}) for Virtual Screening and Affinity Prediction},
  volume = {26},
  issn = {0920-654X, 1573-4951},
  doi = {10.1007/s10822-011-9529-7},
  abstract = {We carried out a prospective evaluation of the utility of the SIE (solvation interaction energy) scoring function for virtual screening and binding affinity prediction. Since experimental structures of the complexes were not provided, this was an exercise in virtual docking as well. We used our exhaustive docking program, Wilma, to provide high-quality poses that were rescored using SIE to provide binding affinity predictions. We also tested the combination of SIE with our latest solvation model, first shell of hydration (FiSH), which captures some of the discrete properties of water within a continuum model. We achieved good enrichment in virtual screening of fragments against trypsin, with an area under the curve of about 0.7 for the receiver operating characteristic curve. Moreover, the early enrichment performance was quite good with 50\% of true actives recovered with a 15\% false positive rate in a prospective calculation and with a 3\% false positive rate in a retrospective application of SIE with FiSH. Binding affinity predictions for both trypsin and host\textendash{}guest complexes were generally within 2 kcal/mol of the experimental values. However, the rank ordering of affinities differing by 2 kcal/mol or less was not well predicted. On the other hand, it was encouraging that the incorporation of a more sophisticated solvation model into SIE resulted in better discrimination of true binders from binders. This suggests that the inclusion of proper Physics in our models is a fruitful strategy for improving the reliability of our binding affinity predictions.},
  language = {en},
  timestamp = {2016-09-16T18:59:47Z},
  number = {5},
  urldate = {2016-09-16},
  journal = {J Comput Aided Mol Des},
  author = {Sulea, Traian and Hogues, Herv{\'e} and Purisima, Enrico O.},
  month = dec,
  year = {2011},
  keywords = {docking,FiSH,SAMPL,SAMPL3,trypsin},
  pages = {617--633},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/JFT7685H/Sulea et al. - 2011 - Exhaustive search and solvated interaction energy .pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/NE5PCP58/s10822-011-9529-7.html:text/html},
  groups = {SAMPL R01,SAMPL R01,SAMPL}
}

@article{beckstein_prediction_2011,
  title = {Prediction of Hydration Free Energies for Aliphatic and Aromatic Chloro Derivatives Using Molecular Dynamics Simulations with the {{OPLS}}-{{AA}} Force Field},
  volume = {26},
  issn = {0920-654X, 1573-4951},
  doi = {10.1007/s10822-011-9527-9},
  abstract = {All-atom molecular dynamics computer simulations were used to blindly predict the hydration free energies of a range of chloro-organic compounds as part of the SAMPL3 challenge. All compounds were parameterized within the framework of the OPLS-AA force field, using an established protocol to compute the absolute hydration free energy via a windowed free energy perturbation approach and thermodynamic integration. Three different approaches to deriving partial charge parameters were pursued: (1) using existing OPLS-AA atom types and charges with minor adjustments of partial charges on equivalent connecting atoms; (2) calculation of quantum mechanical charges via geometry optimization, followed by electrostatic potential (ESP) fitting, using Jaguar at the LMP2/cc-pVTZ(-F) level; and (3) via geometry optimization and CHelpG charges (Gaussian03 at the HF/6-31G* level), followed by two-stage RESP fitting. Protocol 3 generated the most accurate predictions with a root mean square (RMS) error of 1.2kcalmol-11.2kcalmol-11.2$\backslash$,$\backslash$hbox\{kcal\}$\backslash$,$\backslash$hbox\{mol\}\^\{-1\} for the entire data set. It was found that the deficiency of the standard OPLS-AA parameters, protocol 1 (RMS error 2.4kcalmol-12.4kcalmol-12.4$\backslash$,$\backslash$hbox\{kcal\}$\backslash$,$\backslash$hbox\{mol\}\^\{-1\} overall), was mostly due to compounds with more than three chlorine substituents on an aromatic ring. For this latter subset, the RMS errors were 1.4kcalmol-11.4kcalmol-11.4$\backslash$,$\backslash$hbox\{kcal\}$\backslash$,$\backslash$hbox\{mol\}\^\{-1\} (protocol 3) and 4.3kcalmol-14.3kcalmol-14.3$\backslash$,$\backslash$hbox\{kcal\} $\backslash$, $\backslash$hbox\{mol\}\^\{-1\} (protocol 1), respectively. We propose new OPLS-AA atom types for aromatic carbon and chlorine atoms in rings with $\geq$4 Cl-substituents that perform better than the best QM-based approach, resulting in an RMS error of 1.2kcalmol-11.2kcalmol-11.2$\backslash$,$\backslash$hbox\{kcal\} $\backslash$,$\backslash$hbox\{mol\}\^\{-1\} for these difficult compounds.},
  language = {en},
  timestamp = {2016-09-16T18:59:54Z},
  number = {5},
  urldate = {2016-09-16},
  journal = {J Comput Aided Mol Des},
  author = {Beckstein, Oliver and Iorga, Bogdan I.},
  month = dec,
  year = {2011},
  keywords = {hydration free energy,OPLS-AA,SAMPL,SAMPL3},
  pages = {635--645},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/3ANVRFS5/Beckstein and Iorga - 2011 - Prediction of hydration free energies for aliphati.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/CBPIATBH/s10822-011-9527-9.html:text/html},
  groups = {SAMPL R01,SAMPL R01,SAMPL}
