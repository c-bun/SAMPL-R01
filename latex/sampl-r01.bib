  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/KEWNBPNM/Simonson and Roux - 2016 - Concepts and protocols for electrostatic free ener.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/AMT68NT5/08927022.2015.html:text/html},
