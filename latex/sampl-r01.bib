@article{Prill:2011:Sci.Signal.,
  title = {Crowdsourcing {{Network Inference}}: {{The DREAM Predictive Signaling Network Challenge}}},
  volume = {4},
  copyright = {Copyright \textcopyright{} 2011, American Association for the Advancement of Science},
  issn = {1945-0877, 1937-9145},
