  keywords = {force field,machine learning,neural network,Physics - Chemical Physics},
  file = {arXiv\:1610.08935 PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/2PP6BIQK/Smith et al. - 2016 - ANI-1 An extensible neural network potential with.pdf:application/pdf;arXiv.org Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/TD59QZFN/1610.html:text/html},
