  keywords = {hydration free energy,Monte Carlo,QM/MM,SAMPL,SAMPL4},
  pages = {187--200},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/NWR82RV4/Genheden et al. - 2014 - Extensive all-atom Monte Carlo sampling and QMMM .pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/FFR4ERPU/s10822-014-9717-3.html:text/html},
