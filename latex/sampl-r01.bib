  month = dec,
  year = {2011},
  keywords = {Electrostatics,ionizable,pH,pKa values,Proteins,simulation,staphylococcal nuclease},
  pages = {3249--3259},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/5A3UZDFS/Nielsen et al. - 2011 - The pKa Cooperative A collaborative effort to adv.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/ANI7TT36/abstract.html:text/html},
