  journal = {J Comput Aided Mol Des},
  author = {Voet, Arnout R. D. and Kumar, Ashutosh and Berenger, Francois and Zhang, Kam Y. J.},
  month = jan,
  year = {2014},
  keywords = {docking,HIV integrase,human,in cerebro,SAMPL,SAMPL4},
  pages = {363--373},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/G58QZQ26/Voet et al. - 2014 - Combining in silico and in cerebro approaches for .pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/3MVIZRTC/s10822-013-9702-2.html:text/html},
  groups = {SAMPL R01,SAMPL R01,SAMPL,BLUES1,BLUES1}
