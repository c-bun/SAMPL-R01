  author = {Pickard, Frank C. and K{\"o}nig, Gerhard and Tofoleanu, Florentina and Lee, Juyong and Simmonett, Andrew C. and Shao, Yihan and Ponder, Jay W. and Brooks, Bernard R.},
  month = sep,
  year = {2016},
  pages = {1--14},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/WSPETHV7/Pickard et al. - 2016 - Blind prediction of distribution in the SAMPL5 cha.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/TB7HM7QS/s10822-016-9955-7.html:text/html},
