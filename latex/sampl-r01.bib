  pages = {721--733},
  file = {ACS Full Text PDF w/ Links:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/RPZZP3EZ/Lee et al. - 2016 - Simulation-Based Approaches for Determining Membra.pdf:application/pdf;ACS Full Text Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/4AKNNWJV/acs.jcim.html:text/html},
