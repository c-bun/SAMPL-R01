  author = {Reif, Maria M. and Oostenbrink, Chris},
  month = jan,
  year = {2014},
  keywords = {charge corrections,charging free energies,Computer simulation,electrostatic artifacts,free-energy calculations,Molecular dynamics},
  pages = {227--243},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/9SIGEWZP/Reif and Oostenbrink - 2014 - Net charge changes in the calculation of relative .pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/RMWGK3HQ/abstract.html:text/html},
