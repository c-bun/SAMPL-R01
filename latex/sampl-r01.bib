  keywords = {Free energy,hydration free energy,implicit solvent,SAMPL,SEA,transfer},
  pages = {3234--3239},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/HF4KEG7J/Fennell et al. - 2011 - Modeling aqueous solvation with semi-explicit asse.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/TR3B2C5W/3234.html:text/html},
  groups = {SAMPL R01,SAMPL R01,SAMPL},
