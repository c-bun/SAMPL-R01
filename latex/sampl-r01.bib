  urldate = {2016-09-29},
  journal = {J Comput Aided Mol Des},
  author = {Lee, Juyong and Tofoleanu, Florentina and Pickard, Frank C. and K{\"o}nig, Gerhard and Huang, Jing and Damjanovi{\'c}, Ana and Baek, Minkyung and Seok, Chaok and Brooks, Bernard R.},
  month = sep,
